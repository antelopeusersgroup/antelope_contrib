originator     Boulder Real Time Technologies
last_modified  2025-08-01

category       Quanterra
configuration  Q8 Internal Accelerometer Linear Phase Composite

description    Q8 Internal Accelerometer Linear Phase Composite
dfile          q8_int_lc

info    &Literal{
This is the configuration for a Q8 datalogger internal accelerometer.
}

streams        200sps 100sps 50sps 40sps 20sps 10sps 1sps 

#definitions of streams
200sps	200	a/d200	FTA-Linear-200
100sps	100	a/d100	FTA-Linear-100
50sps	50	a/d50	FTA-Linear-50
40sps	40	a/d40	FTA-Linear-40
20sps	20	a/d20	FTA-Linear-20
10sps	10	a/d10	FTA-Linear-10
1sps	1	a/d1	FTA-Linear-1

gains 419463  # in units of counts/volt

# Stages
a/d200	&Arr{
samprate	200
gtype		digitizer
iunits		V
ounits		counts
sn			datalogger
}

a/d100	&Arr{
samprate	100
gtype		digitizer
iunits		V
ounits		counts
sn			datalogger
}

a/d50	&Arr{
samprate	50
gtype		digitizer
iunits		V
ounits		counts
sn			datalogger
}

a/d40	&Arr{
samprate	40
gtype		digitizer
iunits		V
ounits		counts
sn			datalogger
}

a/d20	&Arr{
samprate	20
gtype		digitizer
iunits		V
ounits		counts
sn			datalogger
}

a/d10	&Arr{
samprate	10
gtype		digitizer
iunits		V
ounits		counts
sn			datalogger
}

a/d1	&Arr{
samprate	1
gtype		digitizer
iunits		V
ounits		counts
sn			datalogger
}

FTA-Linear-200	&Arr{
gtype		FIR_decimator
decifac		1
response	&datafile(responses/FTA-Linear-200)
samprate	200
}

FTA-Linear-100	&Arr{
gtype		FIR_decimator
decifac		1
response	&datafile(responses/FTA-Linear-100)
